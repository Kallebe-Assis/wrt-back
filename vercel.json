{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": null,
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/auth/login.js": {
      "maxDuration": 30
    },
    "api/auth/register.js": {
      "maxDuration": 30
    },
    "api/notas.js": {
      "maxDuration": 30
    },
    "api/notas/[id].js": {
      "maxDuration": 30
    },
    "api/links.js": {
      "maxDuration": 30
    },
    "api/links/[id].js": {
      "maxDuration": 30
    },
    "api/admin-categorias.js": {
      "maxDuration": 60
    },
    "api/health.js": {
      "maxDuration": 10
    },
    "api/sync/status.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, PATCH, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, user-id, admin-key"
        }
      ]
    }
  ]
}
